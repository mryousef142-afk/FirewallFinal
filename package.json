{
    "name":  "tg-firewall-bot",
    "private":  true,
    "version":  "1.0.0",
    "type":  "module",
    "homepage":  "https://your-worker-domain.workers.dev",
    "scripts":  {
                    "deploy":  "gh-pages -d dist",
                    "dev":  "vite",
                    "dev:https":  "cross-env HTTPS=true vite",
                    "build":  "tsc --noEmit \u0026\u0026 vite build",
                    "migrate:dev":  "prisma migrate dev",
                    "migrate:deploy":  "prisma migrate deploy",
                    "prisma:studio":  "prisma studio",
                    "lint":  "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
                    "lint:fix":  "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
                    "preview":  "vite preview",
                    "predeploy":  "npm run build",
                    "start":  "tsx server/index.ts",
                    "bot":  "tsx bot/polling.ts",
                    "bot:webhook":  "tsx server/index.ts",
                    "worker:dev":  "wrangler dev",
                    "worker:deploy":  "wrangler deploy",
                    "worker:publish":  "wrangler deploy",
                    "db:backup":  "bash ./scripts/backup-db.sh",
                    "db:backup:ps":  "powershell -ExecutionPolicy Bypass -File ./scripts/backup-db.ps1",
                    "test":  "vitest"
                },
    "dependencies":  {
                         "@prisma/client":  "^5.22.0",
                         "@telegram-apps/sdk-react":  "^3.3.6",
                         "@telegram-apps/telegram-ui":  "^2.1.9",
                         "@tonconnect/ui-react":  "^2.2.0",
                         "dotenv":  "^17.2.3",
                         "eruda":  "^3.4.3",
                         "express":  "^4.21.1",
                         "lottie-react":  "^2.4.1",
                         "p-queue":  "^7.4.1",
                         "pako":  "^2.1.0",
                         "react":  "^18.3.1",
                         "react-dom":  "^18.3.1",
                         "react-router-dom":  "^6.30.1",
                         "telegraf":  "^4.16.3"
                     },
    "devDependencies":  {
                            "@eslint/js":  "^9.23.0",
                            "@types/express":  "^4.17.21",
                            "@types/node":  "^20.19.11",
                            "@types/pako":  "^2.0.4",
                            "@types/react":  "^18.3.24",
                            "@types/react-dom":  "^18.3.7",
                            "@typescript-eslint/eslint-plugin":  "^8.28.0",
                            "@typescript-eslint/parser":  "^8.28.0",
                            "@vitejs/plugin-react-swc":  "^3.11.0",
                            "cross-env":  "^7.0.3",
                            "eslint":  "^9.34.0",
                            "eslint-plugin-react":  "^7.37.5",
                            "eslint-plugin-react-hooks":  "^5.2.0",
                            "gh-pages":  "^6.3.0",
                            "globals":  "^15.15.0",
                            "prisma":  "^5.22.0",
                            "tsx":  "^4.20.6",
                            "typescript":  "^5.9.2",
                            "typescript-eslint":  "^8.40.0",
                            "vite":  "^6.3.5",
                            "vite-plugin-mkcert":  "^1.17.8",
                            "vite-tsconfig-paths":  "^5.1.4",
                            "wrangler":  "^4.42.2",
                            "vitest":  "^1.5.0"
                        }
}
