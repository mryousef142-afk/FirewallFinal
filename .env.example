# Telegram bot token from BotFather
BOT_TOKEN=YOUR_BOT_TOKEN

# Optional: public username without @
BOT_USERNAME=YourBotUsername

# Required: URL of the deployed Telegram mini app
MINI_APP_URL=https://your.domain/mini-app

# Optional: direct channel link
CHANNEL_URL=https://t.me/your_channel

# Optional: override default add-to-group link
ADD_TO_GROUP_URL=

# Optional: tweak the startgroup payload
START_PAYLOAD=fw01

# Mini App Dashboard
VITE_INVITE_LINK=https://t.me/YourBotUsername?startgroup=fw01
VITE_DASHBOARD_DELAY_MS=300
VITE_DASHBOARD_REFRESH_MS=0
